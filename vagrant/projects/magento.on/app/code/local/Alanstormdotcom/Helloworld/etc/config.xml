<?xml version="1.0"?>
<config>
	<modules>
		<Alanstormdotcom_Helloworld>
			<version>0.1.0</version>
		</Alanstormdotcom_Helloworld>
	</modules>
	<global>
		<models>
			<!--roimagic>
				<class>Companyname_RoiMagic_Model</class>
			</roimagic>
			<checkout>
				<rewrite>
					<cart>Companyname_RoiMagic_Model_Cart</cart>
				</rewrite>
			</checkout-->
			<helloworld>
				<class>Alanstormdotcom_Helloworld_Model</class>
			</helloworld>
		</models>
		<helpers>
			<helloworld>
				<class>Alanstormdotcom_Helloworld_Helper</class>
			</helloworld>
		</helpers>
	</global>
	<adminhtml>
		<acl>
			<resources>
				<admin>
					<children>
						<system>
							<children>
								<config>
									<children>
										<helloworld_options>
											<title>Store Hello World Module Section</title>
										</helloworld_options>
									</children>
								</config>
							</children>
						</system>
					</children>
				</admin>
			</resources>
		</acl>
	</adminhtml>
	<frontend>
		<routers>
			<helloworld>
				<use>standard</use>
				<args>
					<module>Alanstormdotcom_Helloworld</module>
					<frontName>helloworld</frontName>
				</args>
			</helloworld>
		</routers>
	</frontend>
</config>